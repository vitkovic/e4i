<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity ServiceOfferRespond.
    -->
    <changeSet id="20230622063629-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="offer_answer_id"
                                 baseTableName="service_offer_respond"
                                 constraintName="fk_service_offer_respond_offer_answer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="answer_to_offer"/>

        <addForeignKeyConstraint baseColumnNames="request_answer_id"
                                 baseTableName="service_offer_respond"
                                 constraintName="fk_service_offer_respond_request_answer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="request_answer"/>

        <addForeignKeyConstraint baseColumnNames="responds_to_request_id"
                                 baseTableName="service_offer_respond"
                                 constraintName="fk_service_offer_respond_responds_to_request_id"
                                 referencedColumnNames="id"
                                 referencedTableName="request_respond"/>

    </changeSet>
</databaseChangeLog>

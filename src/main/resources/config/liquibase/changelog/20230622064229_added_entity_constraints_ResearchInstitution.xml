    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity ResearchInstitution.
    -->
    <changeSet id="20230622064229-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="research_institution_id"
                                 baseTableName="research_institution_researchers"
                                 constraintName="fk_research_institution_researchers_research_institution_id"

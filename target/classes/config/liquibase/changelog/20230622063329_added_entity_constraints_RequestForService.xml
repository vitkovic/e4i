<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity RequestForService.
    -->
    <changeSet id="20230622063329-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="services_id"
                                 baseTableName="request_for_service"
                                 constraintName="fk_request_for_service_services_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ri_service"/>

        <addForeignKeyConstraint baseColumnNames="users_id"
                                 baseTableName="request_for_service"
                                 constraintName="fk_request_for_service_users_id"
                                 referencedColumnNames="id"
                                 referencedTableName="portal_user"/>

        <addForeignKeyConstraint baseColumnNames="organizations_id"
                                 baseTableName="request_for_service"
                                 constraintName="fk_request_for_service_organizations_id"
                                 referencedColumnNames="id"
                                 referencedTableName="portal_user_organization"/>

    </changeSet>
</databaseChangeLog>
